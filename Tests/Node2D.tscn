code = "shader_type canvas_item;

uniform float time;
uniform float radius;
uniform sampler2D bg;
uniform float dist;
uniform float ratio;
uniform vec2 velocity;

uniform vec4 black: hint_color;
uniform vec4 white: hint_color;

void fragment () {
	vec2 center = vec2(0.5);
	float x = length(UV - center);
	vec4 color;
	vec2 uv;
	
	if (length(COLOR - black) < 0.1) {
		uv = UV + velocity*time;
		uv.x += dist*ratio;
		uv = mod(uv,vec2(1.0));
		color = texture(bg, uv);
	}
	else if (length(COLOR - white) < 0.1) {
		color = white;
	}
	
	COLOR = color;
}


"

